menu "ESP TOIT"
    config ESP32_WIFI_SSID
        string "WiFi SSID"
        default "myssid"
        help
            SSID (network name) for the example to connect to.

    config ESP32_WIFI_PASSWORD
        string "WiFi Password"
        default "mypassword"
        help
            WiFi password (WPA or WPA2) for the example to use.

    config ESP_TOIT_USE_JAGUAR
        boolean "Use Jaguar"
        default true
        help
            Use Jaguar on the device to enable fast reload of toit application code.
endmenu